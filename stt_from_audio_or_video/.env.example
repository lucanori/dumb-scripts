# Transcription provider: "groq" or "mistral"
TRANSCRIPTION_PROVIDER=groq

# Add your Groq API key here (required if using groq provider)
# You can obtain one from https://console.groq.com/keys
GROQ_API_KEY=YOUR_GROQ_API_KEY_HERE

# Groq-specific settings (optional)
# GROQ_MODEL=whisper-large-v3-turbo   # Model to use for transcription
# GROQ_LANGUAGE=en                    # Language code (e.g., en, es, fr)
# GROQ_TEMPERATURE=0.0                # Temperature for transcription (0.0-1.0)

# Add your Mistral API key here (required if using mistral provider)
# You can obtain one from https://console.mistral.ai/
MISTRAL_API_KEY=YOUR_MISTRAL_API_KEY_HERE

# Mistral-specific settings (optional)
# MISTRAL_MODEL=voxtral-small-2507    # Model to use for transcription
# MISTRAL_LANGUAGE=en                 # Language code (e.g., en, es, fr)
# MISTRAL_TEMPERATURE=0.0             # Temperature for transcription (0.0-1.0)